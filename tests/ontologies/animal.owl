<rdf:RDF xmlns="http://example.org/simple_ontology#"
     xml:base="http://example.org/simple_ontology"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <owl:Ontology rdf:about="http://example.org/simple_ontology"/>

    <!-- Classes -->
    <owl:Class rdf:about="#Animal"/>
    <owl:Class rdf:about="#Mammal">
        <rdfs:subClassOf rdf:resource="#Animal"/>
    </owl:Class>
    <owl:Class rdf:about="#Dog">
        <rdfs:subClassOf rdf:resource="#Mammal"/>
    </owl:Class>
    <owl:Class rdf:about="#Cat">
        <rdfs:subClassOf rdf:resource="#Mammal"/>
    </owl:Class>
</rdf:RDF>
